{
	"name": "chatdown",
	"displayName": "ChatDown",
	"description": "Visual Studio Code Extension for ChatDown",
	"version": "1.0.1",
	"engines": {
		"vscode": "^1.30.0"
	},
	"publisher":"luisalvesmartins",
	"categories": [
		"Other"
	],
	"repository":"https://github.com/luisalvesmartins/vschatdown",
	"activationEvents": [
		"onCommand:extension.chatdown"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.chatdown",
				"title": "ChatDown"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"eslint": "^4.11.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}
